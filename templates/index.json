    "multicolumn_fF8Vx4": {
      "type": "multicolumn",
      "blocks": {
        "column_NL4H3z": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Fresh_Harvest_cd501e5e-e8aa-4797-a052-896d577d3934.png",
            "title": "Grown Today, Delivered Tomorrow",
            "text": "<p>We minimize transit times and strive to be the fastest farm-to-table solution making our rotating selection of seasonal produce reaches you at their peak of nutrition and flavor<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_YH8mat": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Large_Range_d1f22646-8de6-4004-beaa-ccf1a3eaa716.png",
            "title": "Not Your Average Grocery Delivery",
            "text": "<p>Discover our vast range sustainable groceries, thoughtfully sourced with care and expertise. By eliminating the middleman, we provide you with better value while ensuring farmers and makers receive a fairer share.<\/p>",
            "link_label": "",
            "link": ""
          }
        },
        "column_EUkdzU": {
          "type": "column",
          "settings": {
            "image": "shopify:\/\/shop_images\/Sustainable_7a47787d-a58b-4c56-9903-d355c6a9cdfb.png",
            "title": "Focused on Sustainability",
            "text": "<p>We club deliveries, promote thoughtful ordering, manage inventory precisely, and upcycle produce. Our use of electric vehicles further reduces emissions, ensuring fresher, more sustainable groceries for you.<\/p>",
            "link_label": "",
            "link": ""
          }
        }
      },
      "block_order": [
        "column_NL4H3z",
        "column_YH8mat",
        "column_EUkdzU"
      ],
