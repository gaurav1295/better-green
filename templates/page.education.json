    "rich_text_6jyxnA": {
      "type": "rich-text",
      "blocks": {
        "heading_UMk9PN": {
          "type": "heading",
          "settings": {
            "heading": "IT START AT SCHOOL",
            "heading_size": "h0"
          }
        },
        "caption_YDtyUr": {
          "type": "caption",
          "settings": {
            "caption": "Most of us form our relationship with food at school, but our education system is failing our future generations on two fronts. With food and nature learning absent from the curriculum as well as the cafeteria, children have little opportunity to develop habits that are better for their health and the planet.",
            "text_style": "subtitle",
            "text_size": "medium"
          }
        }
      },
